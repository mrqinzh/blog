<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mrqinzh.blog.mapper.FileMapper">

    <insert id="add" parameterType="com.mrqinzh.blog.model.entity.MyFile">
        INSERT INTO file (
            file_name,
            file_path,
            file_type,
            file_create_time,
            file_size
        )
        VALUES
        (
            #{fileName}, #{filePath}, #{fileType}, #{fileCreateTime}, #{fileSize}
        )
    </insert>

</mapper>